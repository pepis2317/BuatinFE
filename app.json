{
  "expo": {
    "name": "Buatin",
    "slug": "Buatin",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#212732"
    },
    "ios": {
      "supportsTablet": true,
      "config": {
        "googleMapsApiKey": "AIzaSyDg7Bxr3Z2iaOWilJGAPR3xrhgoFJinl9E"
      },
      "bundleIdentifier": "com.pepis2317.BuatinFE"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#222831"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDg7Bxr3Z2iaOWilJGAPR3xrhgoFJinl9E"
        }
      },
      "windowSoftInputMode": "adjustResize",
      "backgroundColor": "#222831",
      "edgeToEdgeEnabled": true,
      "package": "com.pepis2317.BuatinFE",
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "androidNavigationBar": {
      "backgroundColor": "#121212",
      "barStyle": "light-content"
    },
    "androidStatusBar": {
      "backgroundColor": "#121212",
      "barStyle": "light-content",
      "translucent": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-maps",
        {
          "requestLocationPermission": true,
          "locationPermission": "Allow $(PRODUCT_NAME) to use your location"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "df0140e0-6c8b-4b38-93c9-e2c8b9b774d5",
        "googleMapsApiKey": "AIzaSyDg7Bxr3Z2iaOWilJGAPR3xrhgoFJinl9E"
      }
    }
  }
}
